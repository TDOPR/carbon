<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aht.mapper.CoinAddressPoolDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.carbon.entity.CoinAddressPoolEntity" id="BaseResultMap">
        <result property="id" column="id"/>
        <result property="coinId" column="coin_id"/>
        <result property="address" column="address"/>
        <result property="keystore" column="keystore"/>
        <result property="pwd" column="pwd"/>
        <result property="coinType" column="coin_type"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, coin_id,address, keystore, pwd,coin_type
    </sql>
</mapper>